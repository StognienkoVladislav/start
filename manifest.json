{
  "manifest_version": 2,

  "name": "Hello World",
  "description": "Hello world",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "chrome_url_overrides" : {
    "newtab" : "newtab.html"

  },

  "icons"  :{
    "64" : "icon.png"
  },

  "content_scripts" : [
    {
      "matches" : ["http://*/*", "https://*/*"],
      "js" : ["myScript.js"]
    }
  ],
  "background" : {
    "scripts" : ["background.js"]
  }
}
